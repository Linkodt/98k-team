<view class="all"><view class="picker_name"><image class="picker_image" src="../../static/icon/1-8p/icon-locate@2x.png" mode></image><picker mode="selector" range="{{cateen_array}}" value="{{cateen_index}}" data-event-opts="{{[['change',[['picker_change',['$event']]]]]}}" bindchange="__e"><view class="picker_text">{{cateen_array[cateen_index]}}</view></picker></view><view class="top_list"><view class="top_list_view_1"><image class="top_list_img_1" src="../../static/icon/1-8p/461@2x.png" mode="widthFix" data-event-opts="{{[['tap',[['click_img',[0]]]]]}}" bindtap="__e"></image></view><view class="top_list_view_2"><image class="top_list_img_2" src="../../static/icon/1-8p/459@2x.png" mode="widthFix" data-event-opts="{{[['tap',[['click_img',[1]]]]]}}" bindtap="__e"></image></view><view class="top_list_view_3"><image class="top_list_img_3" src="../../static/icon/1-8p/460@2x.png" mode="widthFix" data-event-opts="{{[['tap',[['click_img',[2]]]]]}}" bindtap="__e"></image></view></view><view class="heng"><image class="heng_img" src="../../static/icon/1-8p/headline@2x.png" mode="widthFix"></image></view><block wx:for="{{dish_list}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['naviTo_detail',['$event']]]]]}}" class="dish_list" bindtap="__e"><view class="dish_detail"><image src="{{item.img_src}}" mode></image></view><view class="dish_text"><view class="dish_detail_head">{{''+item.name+''}}</view><view class="dish_detail_text">{{''+item.detail+''}}</view><view class="dish_detail_down"><view class="dish_detail_position">{{''+item.position+"-"+item.window+''}}</view><view class="dish_detail_icon"><view class="dish_detail_rank"><image src="../../static/icon/20-/icon-like-press@2x.png" mode="widthFix"></image><text>{{item.rank}}</text></view><view class="dish_detail_pinglun"><image src="../../static/icon/20-/icon-comment-disable@2x.png" mode="widthFix"></image><text>{{item.pinglun}}</text></view></view></view></view></view></block></view>